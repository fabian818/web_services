<p id="notice"><%= notice %></p>

<h1>Listing My Actions</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Url</th>
      <th>Active</th>
      <th>Provider</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @my_actions.each do |my_action| %>
      <tr>
        <td><%= my_action.name %></td>
        <td><%= my_action.url %></td>
        <td><%= my_action.active %></td>
        <td><%= my_action.provider.name %></td>
        <td><%= link_to 'Show', my_action %></td>
        <td><%= link_to 'Edit', edit_my_action_path(my_action) %></td>
        <td><%= link_to 'Destroy', my_action, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New My action', new_my_action_path %>
